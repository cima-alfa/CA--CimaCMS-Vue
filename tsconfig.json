{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "strict": false,
        "allowJs": true,
        "noImplicitAny": false,
        "types": ["vite/client"],
        "paths": {
            "@/*": ["resources/*"]
        },
        "baseUrl": "."
    },
    "include": [
        "resources/**/*.ts",
        "resources/**/*.d.ts",
        "resources/**/*.vue"
    ]
}
